@{
    ViewData["Title"] = "Home Page";
}

@model HomeViewModel;

@for (int i = 0; i < Model.Map.Dimension; i++)
{
    <br />
    for (int j = 0; j < Model.Map.Dimension; j++)
    {
        <div style="display:inline-block;padding:10px;background-color:@Model.Map.Grid[i,j].Hex"></div>
    }
}